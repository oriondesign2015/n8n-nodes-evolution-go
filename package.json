{
	"name": "n8n-nodes-evolution-go",
	"version": "0.1.2",
	"description": "Evolution Go API is a REST API developed in Go to integrate WhatsApp with applications, offering support for multiple instances and advanced messaging features.",
	"license": "MIT",
	"homepage": "https://github.com/oriondesign2015/n8n-nodes-evolution-go",
	"keywords": [
		"n8n-community-node-package",
		"n8n-nodes-evolution-go",
		"evolution-go",
		"evolution-api"
	],
	"author": {
		"name": "OrionDesign",
		"email": "contato@oriondesign.art.br"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oriondesign2015/n8n-nodes-evolution-go.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
